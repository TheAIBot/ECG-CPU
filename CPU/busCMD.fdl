dp busCMD(in target : ns(4);
		  in RW : ns(1);
		  in address : ns(32);
		  out M_CMD : ns(32))
{
	always
	{
		M_CMD = target # RW # address[0:26];
	}
}

dp testbus(out target : ns(4);
		  out RW : ns(1);
		  out address : ns(32);
		  in M_CMD : ns(32))
{
	always
	{
		target = 0b0100;
		RW = 1;
		address = 0b0101000010000001;
	}
}

