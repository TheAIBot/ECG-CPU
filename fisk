$option "profile_toggle_alledge"
//$option "profile_toggle_upedge"
$option "profile_display_operations"
//$option "profile_display_cycles"

dp fisk(in derp : ns(32); out ll : ns(32)){
	reg kage : ns(32);
	always{
		kage = derp - kage + 57 + (kage >> 1) + (derp << 1);
		ll = kage;
		$display($bin, kage);
	}
}

dp setfisk(in ll : ns(32); out ss : ns(32))
{
	always 
	{
		ss = ll - 3;
	}
}

system lol
{
	setfisk(ll, ee);
	fisk(ee, ll);
}
